	<subDataset name="PieChartDataset" uuid="155c6f37-d504-4c0e-bcfd-a1e645b605bc"/>
	<queryString language="SQL">
		<![CDATA[SELECT
     tbl_producao.`cod_producao` AS tbl_producao_cod_producao,
     tbl_producao.`fk_armazem_fabrica` AS tbl_producao_fk_armazem_fabrica,
     tbl_producao.`produtos` AS tbl_producao_produtos,
     tbl_armazem_fabrica.`cod_armazem` AS tbl_armazem_fabrica_cod_armazem,
     tbl_armazem_fabrica.`fk_fabrica` AS tbl_armazem_fabrica_fk_fabrica,
     tbl_fabrica.`cod_fabrica` AS tbl_fabrica_cod_fabrica,
     tbl_fabrica.`nome_fantasia` AS tbl_fabrica_nome_fantasia,
     tbl_produto.`descricao` AS tbl_produto_descricao,
     tbl_produto.`fk_fabrica` AS tbl_produto_fk_fabrica
FROM
     `tbl_armazem_fabrica` tbl_armazem_fabrica INNER JOIN `tbl_producao` tbl_producao ON tbl_armazem_fabrica.`cod_armazem` = tbl_producao.`fk_armazem_fabrica`
     INNER JOIN `tbl_fabrica` tbl_fabrica ON tbl_armazem_fabrica.`fk_fabrica` = tbl_fabrica.`cod_fabrica`
     INNER JOIN `tbl_produto` tbl_produto ON tbl_fabrica.`cod_fabrica` = tbl_produto.`fk_fabrica`]]>
	</queryString>
